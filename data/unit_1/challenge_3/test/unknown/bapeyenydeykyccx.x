	<h2 class="center">Search the Tameside MBC Website<br />&nbsp;</h2>
	<form method="get" action="/search/index.php">
	<div class="borderdashed aligncenter" style="padding:2px 2px 2px 2px; width:80%">
		<input type="hidden" name="searchtype" value="advanced" />
		<table class="amablue1" cellspacing="0" cellpadding="2" width="100%" summary="Advanced Search">
			<tr	class="bluewhite">
				<th colspan="5">Advanced Search</th>
			</tr>
			<tr valign="top">
				<td rowspan="4"><b><label for="num">Find results</label></b></td>
				<td><label for="as_q">with <b>all</b> of the words</label></td>
				<td>
					<input type="text" name="as_q" id="as_q" class="mandatory" size="25" value=" " />
				</td>
				<td rowspan="4">
					<select name="num" id="num">
						<option value="10" selected="selected">10 results</option>
						<option value="20">20 results</option>
						<option value="30">30 results</option>
						<option value="50">50 results</option>
						<option value="100">100 results</option>
					</select>
				</td>
				<td rowspan="4"><input type="submit" name="btnG" value="Search" class="button" /></td>
			</tr>
			<tr>
				<td><label for="as_epq">with the <b>exact phrase</b></label></td>
				<td><input type="text" name="as_epq" id="as_epq" class="mandatory" size="25" value=" " /></td>
			</tr>
			<tr>
				<td><label for="as_oq">with <b>any</b> of the words</label></td>
				<td><input type="text" name="as_oq" id="as_oq" class="mandatory" size="25" value=" " /></td>
			</tr>
			<tr>
				<td><label for="as_eq"><b>without</b> the words</label></td>
				<td><input type="text" name="as_eq" id="as_eq" class="mandatory" size="25" value=" " /></td>
			</tr>
		</table>
		<table width="80%" summary="Additional Search Parameters">
			<tr>
				<td><b>Occurrences</b></td>
				<td><label for="as_occt">Return results where my terms occur</label></td>
				<td>
					<select name="as_occt" id="as_occt">
						<option value="any" selected="selected">anywhere in the page</option>
						<option value="title">in the title of the page</option>
						<option value="url">in the url of the page</option>
					</select>
				</td>
			</tr>
			<tr>
				<td><b><label for="as_dt">Domains</label></b></td>
				<td>
					<select name="as_dt" id="as_dt">
						<option value="i" selected="selected">Only</option>
						<option value="e">Don't</option>
					</select>
					<label for="as_sitesearch">return results from the site or domain</label>
				</td>
				<td><input type="text" size="25" value=" " name="as_sitesearch" id="as_sitesearch" class="mandatory" /><br /><i>e.g. www.tameside.gov.uk/libraries/</i></td>
			</tr>
			<tr>
				<td><b><label for="sort">Sort</label></b></td>
				<td>
					<select name="sort" id="sort">
						<option value="" selected="selected">by Relevance</option>
						<option value="date:D:S:d1">by Date</option>
					</select>
				</td>
			</tr>
		</table>
	</div>
	<div><br /><br /></div>
</form>
